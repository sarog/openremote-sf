<?xml version="1.0" encoding="UTF-8"?>
<openremote xmlns="http://www.openremote.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openremote.org http://www.openremote.org/schemas/controller.xsd">
	<screens>
		<screen id="5" name="basement" background="basement.png">		  	
		  	<absolute left="20" top="320" width="100" height="100" >
			   <switch id="59" >
							<image src = "a.png" state="ON"/>
							<image src = "b.png" state="OFF"/>
				</switch>
			</absolute>
			<absolute left="20" top="320" width="100" height="100" >
			   <switch id="60" >
							<image src = "c.png" state="ON"/>
							<image src = "d.png" state="OFF"/>
				</switch>
			</absolute>
		</screen>
	  <screen id="6" name="floor" background="basement.png">		  	
		  	<absolute left="20" top="320" width="100" height="100" >
					<switch id="61" >
							<image src = "e.png" state="ON"/>
							<image src = "f.png" state="OFF"/>
				</switch>
			</absolute>
		</screen>
	<screens>
	<groups>
	  <group id="1" name="All rooms">
	    <include type="screen" ref="5" />
	  </group>
	  <group id="2" name="living room">
	    <include type="screen" ref="6" />
	  </group>
	</groups>
</openremote>