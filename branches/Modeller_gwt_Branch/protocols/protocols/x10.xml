<?xml version="1.0" encoding="UTF-8"?>
<protocol name="X10">
       <attr name="address" label="Address">
        <validations>
           <allowBlank>false</allowBlank>
        </validations>
    </attr>
     <attr name="command" label="Command">
        <validations>
           <allowBlank>false</allowBlank>
           <maxLength>10</maxLength>
           <regex>\w*</regex>
        </validations>
    </attr>
</protocol>