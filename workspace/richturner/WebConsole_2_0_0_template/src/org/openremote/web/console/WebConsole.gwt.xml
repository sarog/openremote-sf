<module rename-to='webconsole'>	
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.user.theme.standard.Standard"/>
	
  	<!-- Other module inherits                                      -->
  	<source path='client'/>
  	<source path="type"/>
  	<source path="util"/>
  	<source path="screen"/>
  	<source path="widget"/>
  	<source path="event"/>
	<entry-point class="org.openremote.web.console.client.WebConsole"/>
	
	
	<!-- <set-property name="user.agent" value="safari" /> -->
	<set-property name="user.agent" value="safari" />
</module>

