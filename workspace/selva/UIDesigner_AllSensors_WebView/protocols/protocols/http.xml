<?xml version="1.0" encoding="UTF-8"?>
<openremote xmlns="http://www.openremote.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openremote.org protocol.xsd">
	<protocol displayName="HTTP" tagName="http">
		<attr name="url" label="URL">
			<validations>
				<allowBlank>false</allowBlank>
				<maxLength>254</maxLength>
				<regex>.*</regex>
			</validations>
		</attr>

                <attr name = "username" label = "Username">
                  <validations>
                    <allowBlank>true</allowBlank>
                    <maxLength>254</maxLength>
                    <regex>.*</regex>
                  </validations>
                </attr>

               <attr name = "password" label = "Password">
                 <validations>
                  <allowBlank>true</allowBlank>
                  <maxLength>254</maxLength>
                  <regex>.*</regex>
                </validations>
               </attr>

               <attr name = "command" label = "Command">
                        <validations />
               </attr>

               <attr name = "method" label = "Method">
                        <validations />
               </attr>
	</protocol>
</openremote>
