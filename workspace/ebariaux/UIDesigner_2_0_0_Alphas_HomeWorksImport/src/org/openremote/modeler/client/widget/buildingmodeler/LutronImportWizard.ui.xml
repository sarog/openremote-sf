<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:DialogBox autoHide="false" modal="true">
		<g:caption>
			<b>Lutron HomeWorks import</b>
		</g:caption>
		<g:HTMLPanel>
			<g:FormPanel ui:field="uploadForm">
				<g:VerticalPanel>
					<g:Label ui:field="errorMessageLabel"></g:Label>
					<g:HorizontalPanel>
						<g:FileUpload ui:field="uploadField" name="file" />
						<g:SubmitButton ui:field="submitButton">Import</g:SubmitButton>
					</g:HorizontalPanel>
					<g:Button ui:field="cancelButton">Cancel</g:Button>
				</g:VerticalPanel>

				<!-- <g:DockLayoutPanel unit="EM"> <g:north size="5"> <g:Label ui:field="errorMessageLabel"></g:Label> 
					</g:north> <g:center> <g:HorizontalPanel> <g:FileUpload ui:field="uploadField" 
					name="file" /> <g:SubmitButton ui:field="submitButton">Import</g:SubmitButton> 
					</g:HorizontalPanel> </g:center> <g:south size="5"> <g:Button ui:field="cancelButton">Cancel</g:Button> 
					</g:south> </g:DockLayoutPanel> -->
			</g:FormPanel>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder> 